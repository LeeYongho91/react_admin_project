    "dev": "concurrently \"npm run backend\" \"npm run start --prefix front\"",
    "build": "tsc && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts server/src/",
    "lint:fix": "npm run lint -- --fix",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm i --prefix front && npm run build --prefix front  ",
    "postinstall": "npm run build"
